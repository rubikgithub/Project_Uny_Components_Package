{
    "name": "unygc",
    "version": "0.0.1",
    "type": "module",
    "description": "Distribution files for Uny Components",
    "main": "package/index.umd.js",
    "module": "package/index.es.js",
    "theme":"default",
    "style": "./style.css",
    "exports": {
        ".": {
            "import": "./package/index.es.js",
            "require": "./package/index.umd.js",
            "style":"./Themes/index.css"
        },
        "./style": {
            "default": "./Themes/index.css"
        }
    },
    "bin": {
      "unygc": "./Scripts/install-theme.mjs"
    },
    "script":{
      "themel":"./Scripts/install-theme.mjs",
      "theme":"node node_modules/unygc/Scripts/install-theme.mjs --"
    },  
    
    "files": [
      "package/",
      "Themes/",
      "Scripts/install-theme.mjs",
      "README.md"
    ],
    "dependencies": {
      "@clerk/clerk-js": "^5.29.1",
      "@dnd-kit/core": "^6.1.0",
      "@dnd-kit/sortable": "^8.0.0",
      "@dnd-kit/utilities": "^3.2.2",
      "@emotion/react": "^11.13.3",
      "@emotion/styled": "^11.13.0",
      "@fortawesome/fontawesome-svg-core": "^6.6.0",
      "@fortawesome/free-regular-svg-icons": "^6.6.0",
      "@fortawesome/free-solid-svg-icons": "^6.6.0",
      "@fortawesome/react-fontawesome": "^0.2.2",
      "@mui/material": "^6.1.5",
      "@mui/x-data-grid-premium": "^7.22.0",
      "@syncfusion/ej2-data": "^27.1.52",
      "@syncfusion/ej2-react-schedule": "^27.1.56",
      "@vitejs/plugin-legacy": "^5.4.3",
      "antd": "^5.21.5",
      "axios": "^1.7.7",
      "classnames": "^2.5.1",
      "concurrently": "^6.0.0",
      "core-js": "^3.39.0",
      "cors": "^2.8.5",
      "echarts": "^5.5.1",
      "echarts-for-react": "^3.0.2",
      "express": "^4.21.1",
      "formik": "^2.4.6",
      "json-to-scss": "^1.6.2",
      "jspdf": "^2.5.2",
      "jspdf-autotable": "^3.8.4",
      "moment": "^2.30.1",
      "rc-select": "^14.16.2",
      "react": "^18.3.1",
      "react-beautiful-dnd": "^13.1.1",
      "react-dom": "^18.3.1",
      "react-grid-layout": "^1.5.0",
      "react-icons": "^5.3.0",
      "react-multi-select-component": "^4.3.4",
      "react-redux": "^9.1.2",
      "react-resizable": "^3.0.5",
      "react-router-dom": "^6.27.0",
      "react-split": "^2.0.14",
      "react-virtuoso": "^4.12.0",
      "rsync": "^0.6.1",
      "sass": "^1.80.3",
      "swiper": "^11.1.14",
      "toastr": "^2.1.4"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/rubikgithub/Project_Uny_Components_Package.git"
    },
    "peerDependencies": {
      "react": "^18.0.0",
